#QuestÃ£o 2

def vamos_comprar(N, K, lista):
  compras=[lista[0]]
  for i in range(1,len(lista)):
    contagem=0
    for j in range(0, K):
      if i-j>=0:
        if lista[i] == lista[i-j-1]:
          contagem+=1
    if contagem==0:
      compras.append(lista[i])   
  return len(compras)
print(vamos_comprar(8, 3, [1, 8, 9, 10, 10, 8 ,1]))
